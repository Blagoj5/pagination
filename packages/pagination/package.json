{
  "name": "@bazhe/pagination",
  "version": "1.1.0",
  "description": "Pagination bundle/library that includes multiple pagination packages for react.js and vanilla javascript",
  "author": "Blagoj Petrov",
  "repository": {
    "type": "git",
    "url": "https://github.com/Blagoj5/pagination.git",
    "directory": "packages/pagination"
  },
  "homepage": "https://github.com/Blagoj5/pagination/tree/main/packages/pagination",
